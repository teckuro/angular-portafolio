{
	"version": 2,
	"name": "angular-portafolio",
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist/angular-portafolio"
			}
		}
	],
	"routes": [
		{
			"src": "/sw.js",
			"headers": {
				"Content-Type": "application/javascript"
			}
		},
		{
			"src": "/assets/img/site.webmanifest",
			"headers": {
				"Content-Type": "application/manifest+json"
			}
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	],
	"headers": [
		{
			"source": "/sw.js",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/javascript"
				},
				{
					"key": "Cache-Control",
					"value": "no-cache"
				}
			]
		},
		{
			"source": "/assets/img/site.webmanifest",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/manifest+json"
				}
			]
		}
	]
}
