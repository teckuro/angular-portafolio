{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist/angular-portafolio"
			}
		}
	],
	"routes": [
		{
			"src": "/assets/(.*)",
			"dest": "/assets/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	],
	"env": {
		"NODE_VERSION": "16"
	},
	"functions": {
		"app/api/**/*.js": {
			"runtime": "nodejs16.x"
		}
	}
}
