{
	"version": 2,
	"name": "angular-portafolio",
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist/angular-portafolio"
			}
		}
	],
	"routes": [
		{
			"src": "/sw.js",
			"dest": "/sw.js"
		},
		{
			"src": "/site.webmanifest",
			"dest": "/site.webmanifest"
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	],
	"headers": [
		{
			"source": "/sw.js",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/javascript"
				}
			]
		},
		{
			"source": "/site.webmanifest",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/manifest+json"
				}
			]
		},
		{
			"source": "/(.*\\.js)",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/javascript"
				}
			]
		},
		{
			"source": "/(.*\\.css)",
			"headers": [
				{
					"key": "Content-Type",
					"value": "text/css"
				}
			]
		}
	]
}
