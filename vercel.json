{
	"version": 2,
	"name": "angular-portafolio",
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist/angular-portafolio"
			}
		}
	],
	"routes": [
		{
			"src": "/sw.js",
			"dest": "/sw.js",
			"headers": {
				"Content-Type": "application/javascript"
			}
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	]
}
