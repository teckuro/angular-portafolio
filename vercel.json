{
	"version": 2,
	"name": "angular-portafolio",
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist/angular-portafolio",
				"buildCommand": "npm run build"
			}
		}
	],
	"routes": [
		{
			"src": "/sw.js",
			"dest": "/sw.js",
			"headers": {
				"Content-Type": "application/javascript"
			}
		},
		{
			"src": "/(.*\\.js)",
			"dest": "/$1",
			"headers": {
				"Content-Type": "application/javascript"
			}
		},
		{
			"src": "/(.*\\.css)",
			"dest": "/$1",
			"headers": {
				"Content-Type": "text/css"
			}
		},
		{
			"src": "/(.*\\.json)",
			"dest": "/$1",
			"headers": {
				"Content-Type": "application/json"
			}
		},
		{
			"src": "/(.*\\.webmanifest)",
			"dest": "/$1",
			"headers": {
				"Content-Type": "application/manifest+json"
			}
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	]
}
